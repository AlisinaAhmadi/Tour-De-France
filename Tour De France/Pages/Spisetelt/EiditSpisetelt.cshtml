@page
@model Tour_De_France.Pages.Spisetelt.EiditSpiseteltModel
@{
}
<h1>Edit Spisetelt</h1>

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="Spisetelt.Sid" class="control-lable"></label>
                <input asp-for="Spisetelt.Sid" class="form-control" readonly="@(true)" />
            </div>
            <div class="form-group">
                <label asp-for="Spisetelt.Time" class="control-lable"></label>
                <input asp-for="Spisetelt.Time" class="form-control" />
                <span asp-validation-for="Spisetelt.Time" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Spisetelt.Food" class="control-lable"></label>
                <input asp-for="Spisetelt.Food" class="form-control" />
                <span asp-validation-for="Spisetelt.Food" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Spisetelt.Drinks" class="control-lable"></label>
                <input asp-for="Spisetelt.Drinks" class="form-control" />
                <span asp-validation-for="Spisetelt.Drinks" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Spisetelt.Price" class="control-lable"></label>
                <input asp-for="Spisetelt.Price" class="form-control" />
                <span asp-validation-for="Spisetelt.Price" class="text-danger"></span>
            </div>


            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-page="/Spisetelt/GetAllSpisetelt">Back to List</a>
</div>
