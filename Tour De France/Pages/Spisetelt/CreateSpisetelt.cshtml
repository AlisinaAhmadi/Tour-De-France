@page
@model Tour_De_France.Pages.Spisetelt.CreateSpiseteltModel
@{
}
<h1>CreateSpisetelt</h1>
<body class="zealand">
</body>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="@Model.Spisetelt.Time" class="control-label">Time </label>
                <input asp-for="@Model.Spisetelt.Time " type="time" class="form-control" />

                <span asp-validation-for="@Model.Spisetelt.Time" class="text-danger"></span>

            </div>
            <div class="form-group">
                <label asp-for="@Model.Spisetelt.Food" class="control-label">Food</label>
                <input asp-for="@Model.Spisetelt.Food" class="form-control" />
                <span asp-validation-for="@Model.Spisetelt.Food" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@Model.Spisetelt.Drinks" class="control-label">Drinks</label>
                <input asp-for="@Model.Spisetelt.Drinks" class="form-control" />
                <span asp-validation-for="@Model.Spisetelt.Drinks" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@Model.Spisetelt.Price" class="control-label"> Price </label>
                <input asp-for="@Model.Spisetelt.Price" class="form-control" />
                <span asp-validation-for="@Model.Spisetelt.Price" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
